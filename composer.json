            "src/compact.php",
            "src/compose.php",
            "src/concat.php",
            "src/constant.php",
            "src/contains.php",
            "src/curry.php",
            "src/debounce.php",
            "src/difference.php",
            "src/drop-while.php",
            "src/elem.php",
            "src/equals.php",
            "src/every.php",
            "src/explode.php",
            "src/filter.php",
            "src/find-index.php",
            "src/find.php",
            "src/first-value-with-keys.php",
            "src/first-value.php",
