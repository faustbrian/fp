    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": "vendor/bin/ecs check --fix",
        "refactor": "rector",
        "test": [
            "@test:lint",
            "@test:type-coverage",
            "@test:unit",
            "@test:types",
            "@test:refactor"
        ],
        "test:lint": "vendor/bin/ecs check",
        "test:refactor": "rector --dry-run",
        "test:type-coverage": "php -d memory_limit=512M vendor/bin/pest --parallel --min=90 --type-coverage",
        "test:types": "phpstan",
        "test:unit": "pest --parallel --min=90 --coverage"
    }
}
