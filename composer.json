            "src/head.php",
            "src/headtail.php",
            "src/identity.php",
            "src/implode.php",
            "src/index-by.php",
            "src/init.php",
            "src/intersection.php",
            "src/is-empty.php",
            "src/is-not-empty.php",
            "src/iterate.php",
            "src/itfilter-with-keys.php",
            "src/itfilter.php",
            "src/itmap-with-keys.php",
