{
    "name": "cline/fp",
    "description": "Functional programming utilities for PHP 8.4+ featuring pipe, compose, and lazy evaluation",
    "license": "MIT",
    "keywords": [
        "php",
        "functional-programming",
        "fp",
        "pipe",
        "compose",
        "immutable",
        "lazy-evaluation",
        "functional",
        "array-functions",
        "higher-order-functions"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@cline.sh"
        }
    ],
    "require": {
        "php": "^8.5.0",
        "cline/open-rpc": "^1.1.0",
        "illuminate/bus": "^12.28",
        "illuminate/http": "^12.28",
        "illuminate/support": "^12.28",
        "illuminate/validation": "^12.28",
        "saloonphp/saloon": "^3.14.2",
        "spatie/laravel-data": "^4.18.0"
    },
    "require-dev": {
        "cline/coding-standard": "^1.5.8",
        "orchestra/testbench": "^10.8",
        "symfony/var-dumper": "^7.4.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cline\\fp\\": "src"
        },
